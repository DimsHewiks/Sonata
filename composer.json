{
  "name": "dims-hewik/sonata-fw",
  "description": "Custom PHP Framework",
  "type": "project",
  "require": {
    "php": "^8.4",
    "ext-json": "*",
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Api\\": "api/",
      "Core\\": "core/",
      "View\\": "view/",
      "Command\\": "commands/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
